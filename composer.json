{
    "name": "codelicia/xulieta-json",
    "description": "Xulieta Plugin for JSON validation",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Codelicia\\XulietaJson\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "codelicia/xulieta": "^1.0.0",
        "doctrine/coding-standard": "^9.0.0",
        "phpunit/phpunit": "^9.4"
    },
    "extra": {
        "xulieta": {
            "validator": ["Codelicia\\XulietaJson\\JsonValidator"]
        }
    }
}
